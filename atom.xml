</pre></td><td class="code"><pre>(<span class="function"><span class="keyword">function</span><span class="params">( $ )</span>{</span>
<span class="pi">
  &quot;use strict&quot;</span>;

  $.fn.fitVids = <span class="function"><span class="keyword">function</span><span class="params">( options )</span> {</span>
    <span class="keyword">var</span> settings = {
      customSelector: <span class="literal">null</span>
    };

    <span class="keyword">if</span>(!document.getElementById(<span class="string">&apos;fit-vids-style&apos;</span>)) {

      <span class="keyword">var</span> div = document.createElement(<span class="string">&apos;div&apos;</span>),
          ref = document.getElementsByTagName(<span class="string">&apos;base&apos;</span>)[<span class="number">0</span>] || document.getElementsByTagName(<span class="string">&apos;script&apos;</span>)[<span class="number">0</span>],
          cssStyles = <span class="string">&apos;&#xAD;&lt;style&gt;.fluid-width-video-wrapper{width:100%;position:relative;padding:0;}.fluid-width-video-wrapper iframe,.fluid-width-video-wrapper object,.fluid-width-video-wrapper embed {position:absolute;top:0;left:0;width:100%;height:100%;}&lt;/style&gt;&apos;</span>;

      div.className = <span class="string">&apos;fit-vids-style&apos;</span>;
      div.id = <span class="string">&apos;fit-vids-style&apos;</span>;
