<span class="pi">    &quot;use strict&quot;</span>;

    $(document).ready(<span class="function"><span class="keyword">function</span><span class="params">()</span>{</span>

        $(<span class="string">&quot;.post-content&quot;</span>).fitVids();

    });

}(jQuery));
</pre></td></tr></table></figure>

<figure class="highlight js"><figcaption><span>fitvids.js</span></figcaption><table><tr><td class="gutter"><pre>1
